<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
      <mediaLibrary>
          <mediaTypes>
              <mediaType name="Windows Bitmap image" extensions="bmp, dib">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.ImageMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.ImageMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
              <mediaType name="GIF image" extensions="gif">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.ImageMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.ImageMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
              <mediaType name="JPEG image" extensions="jpg, jpeg, jpe, jfif">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.JpegMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.JpegMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
              <mediaType name="PNG image" extensions="png">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.ImageMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.ImageMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
              <mediaType name="SVG image" extensions="svg">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.SvgMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.SvgMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
              <mediaType name="TIFF image" extensions="tiff, tif">
                  <prototypes>
                      <media patch:instead="*[@type='Sitecore.Resources.Media.ImageMedia, Sitecore.Kernel']" type="Sitecore.Support.Resources.Media.ImageMedia, Sitecore.Support.226960" />
                  </prototypes>
              </mediaType>
          </mediaTypes>
      </mediaLibrary>
  </sitecore>
</configuration>